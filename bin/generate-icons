convert -background none angular/src/assets/icons/template.svg -resize 144x144 angular/src/assets/icons/icon_144.png
convert -background none angular/src/assets/icons/template.svg -resize 180x180 angular/src/assets/icons/icon_180.png
convert -background none angular/src/assets/icons/template.svg -resize 192x192 angular/src/assets/icons/icon_192.png